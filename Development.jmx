        def itFuncId = &quot;&quot;
        def itAcctNum = &quot;&quot;
        def itApprovalReq = &quot;&quot;
        def itEnabled = &quot;&quot;
            def selectFuncAccess = response.Body.FuncsAccessList.FuncAccess.find {
