            if (selectFuncAccess) {
                def limitProperty = &quot;&quot;
                switch (limitPeriod) {
                    case &quot;Per Transaction&quot;:
                        limitProperty = &quot;TrnLimitAmt&quot;
                        break
                    case &quot;Per Day&quot;:
